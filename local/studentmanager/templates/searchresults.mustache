<div> 
{{! class="card" }}
  <br>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Course</th>
        <th>Start Date</th>
        <th>$ (GST exc)</th>
      </tr>
    </thead>
    {{#data}}
      <tr>
        <td>{{{firstname}}}</td>
        <td>{{{lastname}}}</td>
        <td>{{{email}}}</td>
        <td>{{{fullname}}}</td>
        <td>{{{startdate}}}</td>
        <td class="text-left">{{value}}</td>
      </tr>
    {{/data}}
  </table>
    {{^data}}
      <div class="card-body"><p>There are no results for the selected dates.</p></div>
    {{/data}}
</div>
<div>
  <br>
  <br>
    {{#costdata}}
      <h3>${{{test}}}</h3>
    {{/costdata}}
</div>